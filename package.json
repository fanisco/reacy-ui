{
  "name": "reacy-ui",
  "version": "1.0.13",
  "description": "UI kit of React components",
  "author": "Fanil Zubairov <fannisco@gmail.com>",
  "license": "MIT",
  "homepage": "https://fanisco.github.io/reacy-ui",
  "keywords": [
    "react",
    "components"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fanisco/reacy-ui.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile:ts": "tsc",
    "watch:ts": "tsc --watch",
    "compile:css": "npx node-sass --importer node_modules/node-sass-package-importer/dist/cli.js ./src/index.scss -o ./lib",
    "watch:css": "npx node-sass --importer node_modules/node-sass-package-importer/dist/cli.js ./src/index.scss -o ./lib --watch",
    "dist": "npm run compile:ts & npm run compile:css"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "react": "^16.10.2",
    "react-contenteditable": "^3.3.5"
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.11",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "node-sass": "^4.14.1",
    "node-sass-package-importer": "^5.3.2",
    "react-dom": "^16.10.2",
    "react-scripts": "3.2.0",
    "typescript": "3.6.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
