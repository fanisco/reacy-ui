{
  "name": "@fanisco/reacy-ui",
  "version": "0.0.1",
  "description": "UI kit of React components",
  "author": "Fanil Zubairov <fannisco@gmail.com>",
  "license": "MIT",
  "homepage": "https://fanisco.github.io/reacy-ui",
  "keywords": [
    "react",
    "components"
  ],
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fanisco/reacy-ui.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "compile:ts": "tsc",
    "watch:ts": "tsc --watch",
    "compile:css": "npx node-sass --importer node_modules/node-sass-package-importer/dist/cli.js ./src/index.scss -o ./lib",
    "watch:css": "npx node-sass --importer node_modules/node-sass-package-importer/dist/cli.js ./src/index.scss -o ./lib --watch",
    "dist": "npm run compile:ts & npm run compile:css",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "normalize.css": "^8.0.1",
    "react": "^16.10.2",
    "react-contenteditable": "^3.3.5",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.5",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/node-logger": "^6.0.21",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.21",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.11",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "babel-loader": "^8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "node-sass": "^4.14.1",
    "node-sass-package-importer": "^5.3.2",
    "react-dom": "^16.10.2",
    "react-is": "^16.13.1",
    "react-scripts": "3.2.0",
    "typescript": "^3.9.6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
